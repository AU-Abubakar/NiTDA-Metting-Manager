{
  "expo": {
    "name": "MeetingManagerApp",
    "slug": "MeetingManagerApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "CUSTOM: Allow $(PRODUCT_NAME) to access the microphone",
          "speechRecognitionPermission": "CUSTOM: Allow $(PRODUCT_NAME) to securely recognize user speech"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.MeetingManagerApp",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "ios.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "permissions": [
      "android.permission.RECORD_AUDIO"
    ],
    "extra": {
      "eas": {
        "projectId": "3f0fa3e3-20c3-4501-af4c-55b7a83eba83"
      }
    },
    "owner": "aamannari",
    "updates": {
      "url": "https://u.expo.dev/3f0fa3e3-20c3-4501-af4c-55b7a83eba83"
    }
  }
}
